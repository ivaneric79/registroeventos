<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jbandala.registrame" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Registrame</name>
    <description>
       Aplicaci√≥n simple de registro a eventos.
    </description>
    <author email="eric@jbandala.com" href="http://jbandala.com">
       JBandala Consultores
    </author>
    <feature name="Camera">
    <param name="ios-package" value="CDVCamera" />
</feature>
<feature name="NetworkStatus">
    <param name="ios-package" value="CDVConnection" />
</feature>

<feature name="Notification">
    <param name="ios-package" value="CDVNotification" />
</feature>

<icon src="images/icon.png" gap:platform="ios" width="57" height="57" />
<icon src="images/icon-72.png" gap:platform="ios" width="72" height="72" />
<icon src="images/icon-72@2x.png" gap:platform="ios" width="114" height="114" />


    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
</widget>
